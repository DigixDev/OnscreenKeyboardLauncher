<Window x:Class="OnscreenKeyboardLauncher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp38"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <UniformGrid Rows="2">
        <Button Content="Start" x:Name="StartButton" Click="StartButton_OnClick"></Button>
        <Button Content="Close" x:Name="CloseButton" Click="CloseButton_OnClick"></Button>

    </UniformGrid>
</Window>
